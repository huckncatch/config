# work machine profile
echo "oh-my-zsh/custom/profiles/RC6G2R0600"

## load sensitive environment
source $HOME/.dat-creds.zsh

## aws-dat-cli-tool
alias aws-dat-cli="aws_dat_cli"
aws_dat_cli() { $HOME/.aws/aws-dat-cli-tool/dat-kube.sh "$@" fixed args; test -f ~/.dat_kube_config && source ~/.dat_kube_config; }

### Allow AWS SDK to look up config roles when loading profile credentials
export AWS_SDK_LOAD_CONFIG="true"
